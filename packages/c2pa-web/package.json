  "dependencies": {
    "@contentauth/c2pa-wasm": "workspace:*"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/ssri": "^7.1.5",
    "@vitest/browser": "^3.2.4",
    "rimraf": "^6.0.1"
  },
