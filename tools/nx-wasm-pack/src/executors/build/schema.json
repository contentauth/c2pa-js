{
  "$schema": "https://json-schema.org/schema",
  "version": 2,
  "title": "Build executor",
  "description": "",
  "type": "object",
  "properties": {
    "project-dir": {
      "type": "string",
      "description": "Directory of project"
    },
    "profile": {
      "type": "string",
      "enum": [
        "dev",
        "profiling",
        "release"
      ],      
      "default": "release"
    }
  },
  "required": ["project-dir"]
}
